        var clicksDisLike = 0;
    function myFunctionDis() {

        clicksDisLike += 1;
        document.getElementById("demo2").innerHTML = clicksDisLike;
    }
        var clicksDisLike = 0;
    function myFunctionDis() {

        clicksDisLike += 1;
        document.getElementById("demo2").innerHTML = clicksDisLike;
    }
   </script>
